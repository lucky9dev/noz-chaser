{
  "name": "nuzlocke",
  "author": "Diego Ballesteros Castellanos",
  "description": "Nuzlocke tracker to record encounters with detailed information",
  "version": "3.10.0",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/diballesteros/nuzlocke/issues"
  },
  "nyc": {
    "exclude": [
      "src/serviceWorker.ts",
      "src/serviceWorkerRegistration.ts",
      "src/components/UpdateSW/UpdateSW.tsx",
      "src/index.tsx",
      "src/error/ErrorBoundary.tsx"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/service-worker.ts",
      "<rootDir>/src/serviceWorkerRegistration.ts",
      "<rootDir>/components/UpdateSW/UpdateSW.tsx",
      "<rootDir>/src/index.tsx"
    ]
  },
  "private": true,
  "dependencies": {
    "@smogon/calc": "^0.6.0",
    "html-to-image": "^1.9.0",
    "immer": "^9.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.17.5",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-toastify": "^8.0.3",
    "react-window": "^1.8.6",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "zustand": "^3.5.14"
  },
  "scripts": {
    "analyze": "source-map-explorer ''build/static/js/*.js'",
    "start": "react-scripts start",
    "httpss": "HTTPS=true react-scripts start",
    "develop:ci": "CHOKIDAR_USEPOLLING=1 react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "e2e": "cypress open",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x} --ignore-pattern *.test.tsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.11",
    "@cypress/instrument-cra": "^1.4.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.1",
    "@types/react": "^17.0.31",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.1",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "cypress": "^8.6.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-localstorage-commands": "^1.5.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "node-sass": "^6.0.1",
    "prettier": "^2.4.1",
    "sass-loader": "^12.2.0",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.4.4"
  }
}
